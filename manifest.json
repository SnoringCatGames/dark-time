{
  "name": "New Time",
  "version": "0.0.1",
  "description": "Reports how much time you spent visiting certain websites.",
  "permissions": [
    "management",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "page-tracker.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 2
}
